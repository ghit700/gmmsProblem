<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
		<meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
		<title>title</title>
		<link rel="stylesheet" type="text/css" href="../../../css/mui.min.css"/>
		<style>
			body {
			}
			.egret-reflash {
				position: absolute;
			}
			.mui-header {
				background-color: #1081DA
			}
			.mui-header img {
				height: 35px;
				width: 20px;
				padding-top: 10px;
				top: 5px;
				vertical-align: top;
			}
		</style>
	</head>
	<body>
		<header id="header" class="mui-bar mui-bar-nav mui-header">
			<a class="mui-action-menu mui-pull-right egret-reflash" onclick="closeWin()"><img src="../../../image/back_btn.png" /></a>
			<!--<a id="upload" class="mui-action-menu mui-pull-right" onclick="uploadDisease()"><img style="display:none;height: 37px;width: 32px;" class="egret-right" src="../../../image/finish.png" /></a>-->
			<h1 class="mui-title" style="color: white">检查事件</h1>
		</header>
	</body>
	<script type="text/javascript" src="../../../script/api.js"></script>
	<script type="text/javascript" src="../../../script/zepto.min.js"></script>
	<script type="text/javascript" src="../../../script/common.js"></script>
	<script type="text/javascript">
		apiready = function() {
			initHeader();
			api.openFrame({
				name : 'check-event',
				url : './check-event.html',
				rect : {
					x : 0,
					y : $('#header').height(),
					w : 'auto',
					h : 'auto'
				},
				pageParam : {
					"taskId" : api.pageParam.taskId,
					"dpId" : api.pageParam.dpId,
				},
			});
		};
		function uploadDisease() {
			api.execScript({
				frameName : 'check-event',
				script : 'uploadDisease();'
			});
		}



		
	</script>
</html>