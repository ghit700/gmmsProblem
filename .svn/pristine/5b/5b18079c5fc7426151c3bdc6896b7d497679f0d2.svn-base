<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8"/>
		<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
		<meta name="format-detection" content="telephone=no,email=no,date=no,address=no"/>
		<link rel="stylesheet" type="text/css" href="../../../../css/aui.css"/>
	</head>
	<body>
		<header class="aui-bar aui-bar-nav" id="aui-header">
			<a class="aui-btn aui-pull-left" tapmode onclick="closeWin()"> <span class="aui-iconfont aui-icon-left"></span> </a>
			<div class="aui-title">选择功能位置</div>
			<a class="aui-btn aui-pull-right" tapmode onclick="fnSelDevicePlaces()">确定</a>
		</header>
	</body>
	<script type="text/javascript" src="../../../../script/api.js"></script>
	<script type="text/javascript">
		apiready = function() {
			api.parseTapmode();
			var header = $api.byId('aui-header');
			$api.fixStatusBar(header);
			var headerPos = $api.offset(header);
			api.openFrame({
				name : 'sel_device_place_frm',
				url : './sel_device_place_frm.html',
				bounces : false,
				pageParam : api.pageParam,
				rect : {
					x : 0,
					y : headerPos.h,
					w : 'auto',
					h : 'auto'
				}
			});
		};
		
		function fnSelDevicePlaces(){
			api.execScript({
				frameName: 'sel_device_place_frm',
	            script: 'fnSelDevicePlaces();'
            });
		}
		
		function closeWin() {
			api.closeWin();
		}
	</script>
</html>
